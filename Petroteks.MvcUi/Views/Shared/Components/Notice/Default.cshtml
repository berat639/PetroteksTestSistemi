@model ICollection<UI_Notice>

<!-- Modal Structure -->
<div id="modal1" class="modal">
    <div class="modal-content" style="padding:1em!important" >






        <div class="large-12 columns">
            <div class="owl-carousel owl-theme" id="contentify">
                @foreach (var item in Model)
                {
                    <div class="item">
                       @Html.Raw(item.Content)
                    </div>
                }


               
            </div>


        </div>




    </div>
    <a href="#!" style="position: fixed; top: 10px; color: #5d5d5d; font-size: 1.5em; z-index: 1000; right: 10px;" class="modal-close"><i class="fa fa-close"></i></a>
</div>